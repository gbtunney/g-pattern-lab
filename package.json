{
  "name": "g-pattern-lab",
  "version": "1.0.0",
  "description": "Pattern Lab ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-webfont": "cp -a node_modules/@fortawesome/fontawesome-free/webfonts/. source/fonts/",
