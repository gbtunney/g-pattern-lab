<div  class="o-container-hlist">
    {% for key, value in render_component_themes %}

        {% set newdata = data.data %}
        {% set newdata = newdata|merge({ ("styleModifier") : "^" ~ key ~ " --circle"}) %}
        {% set newdata = newdata|merge({ ("parent") : component_id ~ "--" ~ key }) %}

        {% include data.pattern  with  newdata %}


    {% endfor %}
</div>
