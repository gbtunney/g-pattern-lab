{
	"stringtest": "i was here",
	"baseColors": {
		"white": "#fff",
		"black": "#000",
		"dark-gray": "#585858",
		"tana": "#bdba9c",
		"khaki": "#f0e68c",
		"west-coast": "#64551f",
		"highball": "#90813b",
		"sandstone": "#7b6d5c",
		"schooner": "#8b8378",
		"spice": "#66543e",
		"madras": "#4c3b25",
		"onion": "#33250d",
		"woodrush": "#332915",
		"jacko-bean": "#2e1c08",
		"acadia": "#1e1205",
		"crowshead": "#1a130a"
	},
	"schemes": [
		{
			"key": "light",
			"description": "'The main light on dark theme.'",
			"parent": false,
			"colors": {
				"foreground": "woodrush",
				"background": "tana",
				"accent": "schooner",
				"border": "highball",
				"link": "tana"
			}
		},
		{
			"key": "dark",
			"description": "'DARK THEME!!'",
			"parent": false,
			"colors": {
				"foreground": "tana",
				"background": "crowshead",
				"accent": "khaki",
				"border": "foreground",
				"link": "khaki"
			}
		},
		{
			"key": "top-menu",
			"description": "'top menu!!'",
			"parent": "dark",
			"colors": {
				"background": "jacko-bean"
			}
		},
		{
			"key": "form",
			"description": "'top menu!!'",
			"parent": "dark",
			"colors": {
				"border": "sandstone",
				"background": "madras"
			}
		},
		{
			"key": "tabs",
			"description": "'tabs menu!!'",
			"parent": "light",
			"colors": {
				"inactive": "onion"
			}
		},
		{
			"key": "grid-helper",
			"description": "'grid helper menu!!'",
			"parent": "dark",
			"colors": {
				"foreground": "white",
				"background": "madras",
				"accent": "khaki",
				"border": "red"
			}
		},
		{
			"key": "modal",
			"description": "'grid helper menu!!'",
			"parent": "dark",
			"colors": {
				"overlay": "madras",
				"background": "dark-gray"
			}
		}
	],
	"render-component-themes": {
		 "light":"Light",
		"dark": "Dark"
	},
	"headings": {
		"component_id": "g_Heading",
		"modifiers": [
			"2-font-serif",
			"trailing-border",
			"left-border",
			"3"
		]
	},
	"typography": {
		"required-props": "'font-family, letter-spacing, word-spacing, text-transform, text-decoration, font-style, font-weight'",
		"line-height": 1.4,
		"modularscale": {
			"base": "20px",
			"ratio": 1.2,
			"sm": {
				"base": "25px",
				"ratio": 1.2
			}
		},
		"fonts": {
			"default": {
				"font-family": "monospace",
				"text-transform": ".2px",
				"text-decoration": "none",
				"letter-spacing": 0,
				"word-spacing": "-1px",
				"font-weight": "normal",
				"font-style": "normal"
			},
			"font-san-serif": {
				"font-family": "mr-eaves-sans"
			},
			"font-serif": {
				"font-family": "mrs-eaves"
			},
			"font-small-caps": {
				"font-family": "mrs-eaves-roman-small-caps",
				"text-transform": "uppercase"
			}
		}
	},
	"slicer": {
		"breakpoints": "0 480px 768px 992px 1200px",
		"breakpoint-names": "xs sm md lg"
	},
	"paths": {
		"image-assets": "'./public/images/'",
		"icons": "'./public/images/icons/'",
		"patterns": "'./public/images/patterns/'",
		"fonts": "'./public/fonts'"
	}
}