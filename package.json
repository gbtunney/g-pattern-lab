  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "node-sass": "^4.9.2",
    "node-sass-json-importer": "^3.3.1"
  },
  "watch": {
    "test": "{src,test}/*.js"
  },
  "devDependencies": {
    "breakpoint-sass": "^2.7.1",
    "breakpoint-slicer": "^2.0.0",
    "chroma-sass": "^1.2.6",
    "compass-mixins": "^0.12.10",
    "modularscale-sass": "^3.0.5",
    "npm-watch": "^0.3.0",
    "sass-bem-constructor": "^1.1.0",
    "sass-dry": "^1.0.0",
    "sass-toolkit": "^2.10.2"
  }
